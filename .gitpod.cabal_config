repository hackage.haskell.org
  url: http://hackage.haskell.org/

remote-repo-cache: /home/gitpod/.cabal/packages
world-file: /home/gitpod/.cabal/world
extra-prog-path: /home/gitpod/.cabal/bin
build-summary: /home/gitpod/.cabal/logs/build.log
remote-build-reporting: none
jobs: 4
overwrite-policy: always
install-method: copy
installdir: /home/gitpod/.cabal/bin

haddock
  hoogle: True
  html: True
